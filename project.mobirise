          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer2\" group=\"Footers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" global once=\"footers\" mbr-class=\"{\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"../../app/themes/mobirise5/components/_images/background8.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#080f26\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"text\" title=\"Fallback Image\" name=\"videoToImageOnMobile\" condition=\"overlay && bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\" video-to-image-on-mobile=\"{{videoToImageOnMobile}}\">\n        <div class=\"row mbr-white\">\n            <div class=\"col-6 col-md-4 col-lg-3\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                    <b>Mentions légales</b></h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"conditions-generales-utilisation.html\" class=\"text-success\">CGU</a></span></li><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"mentions-legales.html\" class=\"text-success\">Mentions légales</a></span><br></li><a href=\"mentions-legales.html\" class=\"text-primary\">\n                    </a><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"conditions-generales-utilisation.html\" class=\"text-success\">RGPD</a></span><br></li>\n                </ul>\n            </div>\n            <div class=\"col-6 col-md-4 col-lg-3\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                    <b>Menu</b></h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"instrumentation-et-mesures-physiques.html\" class=\"text-success\">Mesures</a></span></li><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"equipe.html\" class=\"text-success\">Equipe</a></span></li><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"contact.html\" class=\"text-success text-primary\">Contact</a></span></li><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"fiabilite-des-tests-produits.html\" class=\"text-success\">Fiabilité des tests produits</a><br></span><br></li>\n                </ul>\n            </div>\n            <div class=\"col-6 col-md-4 col-lg-3\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                    <b>Derniers articles</b></h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"biais-de-confirmation.html\" class=\"text-success\">Biais de confirmation</a></span></li><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"panel-sensoriel.html\" class=\"text-success\">Panel sensoriel</a></span></li><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"panels-testeurs-produits.html\" class=\"text-success\">Types de panels&nbsp;</a></span></li><li class=\"mbr-text item-wrap\"><span style=\"font-size: 1.2rem; text-align: var(--bs-body-text-align);\"><a href=\"effet-de-simple-exposition.html\" class=\"text-success\">Effet de simple exposition</a><br><br><br></span></li><li class=\"mbr-text item-wrap\"><br></li>\n                </ul>\n            </div>\n            <div class=\"col-6 col-md-4 col-lg-3\">\n                <h5 mb